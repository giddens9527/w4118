<h1>Listing possesses</h1>

<table>
  <tr>
    <th>Skill</th>
    <th>User</th>
    <th>Integer</th>
    <th>Proficiency</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @possesses.each do |possess| %>
  <tr>
    <td><%= possess.skill_id %></td>
    <td><%= possess.user_id %></td>
    <td><%= possess.integer %></td>
    <td><%= possess.proficiency %></td>
    <td><%= link_to 'Show', possess %></td>
    <td><%= link_to 'Edit', edit_possess_path(possess) %></td>
    <td><%= link_to 'Destroy', possess, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Possess', new_possess_path %>
